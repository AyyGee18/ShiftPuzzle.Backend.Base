using System;

class Program
{
    static void Main(string[] args)
    {
        int[] массив = { 10, 20, 30, 40, 50 }; // Инициализация массива

        int сумма = 0; // Переменная для хранения суммы элементов

        foreach (int элемент in массив) // Проходим по всем элементам массива
        {
            сумма += элемент; // Добавляем значение элемента к сумме
        }

        Console.WriteLine("Сумма элементов массива: " + суммаПроизошла ошибка. (Код ошибки MV1K11F21)