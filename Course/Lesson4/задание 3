using System;

class Program
{
    static void Main(string[] args)
    {
        int[] массив = { 1, 2, 3, 4, 5 }; // Инициализация массива

        int длина = массив.Length; // Получаем длину массива

        for (int i = 0; i < длина / 2; i++) // Итерируемся только до половины массива, чтобы поменять местами элементы
        {
            int временный = массив[i]; // Создаем временную переменную для хранения текущего элемента
            массив[i] = массив[длина - 1 - i]; // Меняем текущий элемент соответствующим элементом с другой стороны массива
            массив[длина - 1 - i] = временный; // Присваиваем другому элементу значение временной переменной
        }

        Console.WriteLine("Перевернутый массив: ");
        foreach (int элемент in массив) // Проходим по всем элементам массива
        {
            Console.Write(элемент + " "); // Выводим элементы массива на консоль
        }
    }
}