using System;

class Program
{
    static void Main(string[] args)
    {
        int[] массив = { 10, 20, 30, 40, 50 }; // Инициализация массива

        int максимальное_значение = массив[0]; // Переменная для хранения максимального значения

        foreach (int элемент in массив) // Проходим по всем элементам массива
        {
            if (элемент > максимальное_значение) // Если текущий элемент больше текущего максимального значения
            {
                максимальное_значение = элемент; // Обновляем максимальное значение
            }
        }

        Console.WriteLine("Максимальное значение: " + максимальное_значение);
    }
}