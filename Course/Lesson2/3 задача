using System;

class Program
{
    static void Main(string[] args)
    {
        // Создаем массивы для хранения имен и телефонных номеров
        string[] names = new string[3];
        string[] phoneNumbers = new string[3];

        // Получаем от пользователя информацию и сохраняем ее в массивах
        for (int i = 0; i < 3; i++)
        {
            Console.WriteLine("Введите имя:");
            names[i] = Console.ReadLine();

            Console.WriteLine("Введите телефонный номер:");
            phoneNumbers[i] = Console.ReadLine();
        }

        // Выводим сохраненные данные
        Console.WriteLine("Имена и телефонные номера:");

        for (int i = 0; i < 3; i++)
        {
            Console.WriteLine($"Имя: {names[i]}, Телефонный номер: {phoneNumbers[i]}");
        }

        Console.ReadLine();
    }
}
