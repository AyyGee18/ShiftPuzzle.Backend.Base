using System;

class Program
{
    static void Main()
    {
        // Создаем массив из пяти чисел
        int[] arr = new int[5];

        // Инициализируем элементы массива
        arr[0] = 1;
        arr[1] = 2;
        arr[2] = 3;
        arr[3] = 4;
        arr[4] = 5;

        // Выводим элементы массива
        Console.WriteLine("Элементы массива:");
        foreach (int num in arr)
        {
            Console.WriteLine(num);
        }
    }
}