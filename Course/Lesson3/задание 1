
using System;

class Program
{
    static void Main()
    {
        int[] numbers = new int[5]; // Объявление массива целых чисел длиной 5

        for (int i = 0; i < numbers.Length; i++)
        {
            numbers[i] = i + 1; // Заполнение массива значениями от 1 до 5
        }

        Console.WriteLine("Массив чисел:");

        foreach (int number in numbers)
        {
            Console.WriteLine(number); // Вывод значений массива
        }
    }
}
