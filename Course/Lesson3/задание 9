using System;

class Program
{
    static void Main(string[] args)
    {
        int[] массив = new int[5]; // Создание массива из пяти элементов

        // Вручную заполняем массив значениями
        массив[0] = 10;
        массив[1] = 20;
        массив[2] = 30;
        массив[3] = 40;
        массив[4] = 50;

        Console.WriteLine("Массив:");
        for (int i = 0; i < массив.Length; i++) // Выводим значения массива на консоль
        {
            Console.WriteLine(массив[i]);
        }

        // Добавьте остальную часть кода, которая выполняет нужные вам действия с массивом
        
    }
}